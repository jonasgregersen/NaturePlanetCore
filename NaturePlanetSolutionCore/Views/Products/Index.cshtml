@model List<DataTransferLayer.Model.Product>

<div class="product-container">
    @foreach (var product in @Model)
    {
        <div class="product-@product.Name">
            <a asp-controller="Products" asp-action="Details" asp-route-product="@product.Name">@product.Name</a>
            <p>@product.EAN</p>
        </div>
    }
</div>


