@model List<string>
<<link rel="stylesheet" href="css/navbar.css">
<ul class="navbar-nav me-auto mb-2 mb-lg-0">
    @foreach (var category in Model)
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="Products" asp-action="ByCategory" asp-route-category="@category">
                @category
            </a>
        </li>
    }
</ul>
